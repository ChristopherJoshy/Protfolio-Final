{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/client/assets/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*\\.[a-z0-9]+$)",
      "dest": "/dist/client/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/client/index.html"
    }
  ],
  "env": {
    "DATABASE_URL": "postgresql://neondb_owner:npg_IUcurO9YfXP1@ep-broad-star-a49b8m9i-pooler.us-east-1.aws.neon.tech/neondb?sslmode=require",
    "NODE_ENV": "production",
    "GITHUB_TOKEN": "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3"
  }
}